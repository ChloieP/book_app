<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../layout/head') %>
  </head>

  <body>
    <header>
    <h1>Central Oregon Cool Books!</h1>
    <p>X</p>
    <p><a href="./searches/new">X</a></p>
    
    </header>


    <section class="titleContainer">
      <p><%=results.length%> Saved Books</p>
    </section>

<section class="showContainer">
    <% results.forEach(books => { %>
   <img src="<%= books.image_url %>"  alt="">
   <h2><%= books.title %> </h2>
   <h2><%= books.author %></h2>
   <h2><a href="/books/<%=books.id%>"> View Details</a></h2>
    <% }); %>
</section>

  </body>


</html>